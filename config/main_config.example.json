{
  "_comment": "TradingAgents 配置示例文件",
  "_note1": "复制此文件为 main_config.json 并修改配置",
  "_note2": "环境变量会覆盖此文件中的配置",

  "llm_provider": "dashscope",
  "backend_url": "https://dashscope.aliyuncs.com/api/v1",
  "deep_think_llm": "qwen-plus",
  "quick_think_llm": "qwen-turbo",
  "max_debate_rounds": 1,
  "online_tools": true,

  "parallel_analysts": false,
  "max_parallel_workers": 2,
  "analyst_timeout": 180,

  "_examples": {
    "_comment": "不同LLM提供商的配置示例",

    "dashscope_example": {
      "llm_provider": "dashscope",
      "backend_url": "https://dashscope.aliyuncs.com/api/v1",
      "deep_think_llm": "qwen3-max",
      "quick_think_llm": "qwen-plus"
    },

    "openai_example": {
      "llm_provider": "openai",
      "backend_url": "https://api.openai.com/v1",
      "deep_think_llm": "gpt-4",
      "quick_think_llm": "gpt-3.5-turbo"
    },

    "deepseek_example": {
      "llm_provider": "deepseek",
      "backend_url": "https://api.deepseek.com",
      "deep_think_llm": "deepseek-chat",
      "quick_think_llm": "deepseek-chat"
    }
  },

  "_env_vars": {
    "_comment": "可以通过以下环境变量覆盖配置",
    "llm_provider": "export TA_LLM_PROVIDER=dashscope",
    "backend_url": "export TA_BACKEND_URL=https://dashscope.aliyuncs.com/api/v1",
    "deep_think_llm": "export TA_DEEP_THINK_LLM=qwen3-max",
    "quick_think_llm": "export TA_QUICK_THINK_LLM=qwen-plus",
    "max_debate_rounds": "export TA_MAX_DEBATE_ROUNDS=2",
    "online_tools": "export TA_ONLINE_TOOLS=true",
    "parallel_analysts": "export TA_PARALLEL_ANALYSTS=true",
    "max_parallel_workers": "export TA_MAX_PARALLEL_WORKERS=4",
    "analyst_timeout": "export TA_ANALYST_TIMEOUT=300"
  }
}